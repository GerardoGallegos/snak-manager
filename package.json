{
  "name": "snak-manager",
  "description": "Educational tool to fast and effective learning",
  "version": "0.0.1",
  "author": "Gerardo Gallegos <puxerop@gmail.com>",
  "user": "gerardogallegos",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "build": "webpack",
    "build:production": "webpack --config webpack-production.config.js -p"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/tests/jest-script-preprocessor.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.*[.](jpg|JPG|gif|GIF|png|PNG|less|LESS|css|CSS|scss|SCSS)$": "<rootDir>/tests/EmptyModule.js"
    },
    "preprocessorIgnorePatterns": [
      "/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "enzyme"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^11.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-jest": "^11.0.2",
    "babel-preset-react": "^6.5.0",
    "brace": "^0.8.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "hamsterjs": "^1.1.2",
    "jest": "^0.1.40",
    "jest-cli": "^11.0.2",
    "mousetrap": "^1.5.3",
    "node-sass": "^3.4.2",
    "react": "^15.0.1",
    "react-ace": "^3.2.0",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-dropzone": "^3.4.0",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "uid": "0.0.2",
    "wavesurfer.js": "^1.1.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/GerardoGallegos/snak-manager.git"
  },
  "bugs": {
    "url": "https://github.com/GerardoGallegos/snak-manager/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate"
  ]
}
